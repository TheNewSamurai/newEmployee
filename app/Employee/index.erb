<body>
	<div id="container">
      <% @employee.each do |employee| %>
      
          <li>
            <a href="<%= url_for :action => :show, :id => employee.object %>">
              <%= employee.firstName %> &nbsp <%= employee.lastName %>
            </a>
          </li>
      
      <% end %>
	</div>
